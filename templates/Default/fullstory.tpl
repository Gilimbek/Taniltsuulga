<article id="post-" class="gallery-single post-3660 dt_portfolios type-dt_portfolios status-publish has-post-thumbnail hentry portfolio_entries-facial-therapy portfolio_entries-haircut-styling">
    <div class="recent-gallery-container">
        <ul class="recent-gallery">
            [xfgiven_youtube]
            <li>
                <div class="dt-video-wrap">
                    [xfvalue_youtube]
                </div>
            </li>
            [/xfgiven_youtube]
            [image-1]
            <li><img src="{image-1}"/>
            </li>
            [/image-1]
            [image-2]
            <li><img src="{image-2}"/>
            </li>
            [/image-2]
            [image-3]
            <li><img src="{image-3}"/>
            </li>
            [/image-3]
        </ul>
        
    </div>
    <div style="position:relative;" class="gallery-single-container">
        <div class="dt-sc-hr-invisible-very-very-small"></div>
        <div class="column dt-sc-two-third first">
            <h3>{title}</h3>
            <div class="portfolio-meta-data">
                <h6>{category}</h6>
            </div>
            <p>{full-story}</p>
            
        </div>
        <div class="column dt-sc-one-third">

            <ul class="portfolio-details">
                <li>
                    <span>Огноо :</span> {date=d/m/y}</li>
                <li>
                    <span>Ангилал :</span>
                    <a href="{full-link}" title="View all posts by ">{category}</a>
                </li>
            </ul>
            <div class="dt-sc-hr-invisible-small"></div>
            <ul class="dt-sc-social-icons">
                
                <li><a class="facebook" href="{full-link}" data-href="{full-link}" onclick="javascript:window.open(this.href, '', 'menubar=no,toolbar=no,resizable=yes,scrollbars=yes,height=600,width=600');
                        return false;" target="_blank">
                    <span class="fa fa-facebook"></span>
                    </a>
                </li>
                <li>
                    <a onclick="shindiriShowPopup(this.href);
                        return(false);" class="twitter" href="{title}+{full-link}" data-count-layout="horizontal" target="_blank">
                    	<span class="fa fa-twitter"></span>
                    </a>
                </li>
                <li><a href="#" title="youtube" class="youtube" target="_blank"> <span class="fa fa-youtube"></span></a></li>
                <li>
                    <a onclick="shindiriShowPopup(this.href);
                        return(false);" class="googleplus" href="https://plus.google.com/share?url={full-link}" data-href="{full-link}" data-send="false" data-layout="button_count" data-width="60" data-show-faces="false" target="_blank">
                    	<span class="fa fa-google-plus"></span>
                    </a>
                </li>
            </ul>
        </div>
        <div class="dt-sc-margin60"></div>
    </div>
    <div class="dt-sc-hr-invisible"></div>
</article>